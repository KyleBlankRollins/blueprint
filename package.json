{
  "name": "@krollins/blueprint",
  "version": "0.2.0",
  "description": "A highly portable and customizable component library built on Lit",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./accordion": "./dist/components/accordion.js",
    "./alert": "./dist/components/alert.js",
    "./avatar": "./dist/components/avatar.js",
    "./badge": "./dist/components/badge.js",
    "./breadcrumb": "./dist/components/breadcrumb.js",
    "./button": "./dist/components/button.js",
    "./card": "./dist/components/card.js",
    "./checkbox": "./dist/components/checkbox.js",
    "./color-picker": "./dist/components/color-picker.js",
    "./combobox": "./dist/components/combobox.js",
    "./date-picker": "./dist/components/date-picker.js",
    "./divider": "./dist/components/divider.js",
    "./drawer": "./dist/components/drawer.js",
    "./dropdown": "./dist/components/dropdown.js",
    "./file-upload": "./dist/components/file-upload.js",
    "./heading": "./dist/components/heading.js",
    "./icon": "./dist/components/icon.js",
    "./input": "./dist/components/input.js",
    "./link": "./dist/components/link.js",
    "./menu": "./dist/components/menu.js",
    "./modal": "./dist/components/modal.js",
    "./multi-select": "./dist/components/multi-select.js",
    "./notification": "./dist/components/notification.js",
    "./number-input": "./dist/components/number-input.js",
    "./pagination": "./dist/components/pagination.js",
    "./popover": "./dist/components/popover.js",
    "./progress": "./dist/components/progress.js",
    "./radio": "./dist/components/radio.js",
    "./select": "./dist/components/select.js",
    "./skeleton": "./dist/components/skeleton.js",
    "./slider": "./dist/components/slider.js",
    "./spinner": "./dist/components/spinner.js",
    "./stepper": "./dist/components/stepper.js",
    "./switch": "./dist/components/switch.js",
    "./table": "./dist/components/table.js",
    "./tabs": "./dist/components/tabs.js",
    "./tag": "./dist/components/tag.js",
    "./text": "./dist/components/text.js",
    "./textarea": "./dist/components/textarea.js",
    "./time-picker": "./dist/components/time-picker.js",
    "./tooltip": "./dist/components/tooltip.js",
    "./tree": "./dist/components/tree.js",
    "./icons/*": "./dist/icons/*.js"
  },
  "files": [
    "dist",
    "!dist/cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "dist/index.js",
    "dist/index.css",
    "dist/components/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleBlankRollins/blueprint.git"
  },
  "homepage": "https://github.com/KyleBlankRollins/blueprint#readme",
  "bugs": "https://github.com/KyleBlankRollins/blueprint/issues",
  "scripts": {
    "dev": "cd docs && npm run dev",
    "build": "npm run theme:generate-types && vite build && tsc -p tsconfig.build.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:package": "npm run build && npm pack",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:cli": "vitest source/cli",
    "test:themes": "vitest source/themes",
    "test:themes:run": "vitest run source/themes",
    "lint": "eslint source --ext .ts,.tsx",
    "lint:fix": "eslint source --ext .ts,.tsx --fix",
    "format": "prettier --write source/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check source/**/*.{ts,tsx,css,md}",
    "theme:generate": "npm run build:cli && node dist/cli/commands/generate-theme.js",
    "theme:generate-types": "npm run build:cli && node dist/cli/index.js theme generate-types",
    "theme:watch-types": "npm run build:cli && node dist/cli/index.js theme generate-types --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:icons": "node source/components/icon/generate-icon-entries.js",
    "generate:jsx": "npm run build:cli && node dist/cli/index.js generate jsx",
    "prepack": "node -e \"require('fs').copyFileSync('README.npm.md','README.md')\"",
    "postpack": "git checkout README.md"
  },
  "keywords": [
    "web-components",
    "lit",
    "typescript",
    "components",
    "ui"
  ],
  "author": "Kyle Rollins",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^10.0.1",
    "@iarna/toml": "^2.2.5",
    "@inquirer/prompts": "^8.1.0",
    "@storybook/web-components-vite": "^10.1.11",
    "@types/chokidar": "^1.7.5",
    "@types/culori": "^4.0.1",
    "@types/iarna__toml": "^2.0.5",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "culori": "^4.0.2",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.2.10",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "storybook": "^10.1.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@krollins/blueprint": "^0.2.0",
    "lit": "^3.3.2"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  }
}
